{
  "name": "tankdb",
  "version": "0.9.5",
  "description": "Distributed graph database",
  "main": "tank.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm run build:code",
    "build:code": "esbuild $(aaargh buildconfig.json)",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{print $2}' | sed 's/[\",]//g' | tr -d '[[:space:]]') && npm deprecate \"tankdb@<${PACKAGE_VERSION}\" \"Rolling release, please update to ${PACKAGE_VERSION}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finwo/tank.git"
  },
  "author": "Robin Bron <robin@finwo.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/finwo/tankdb/issues"
  },
  "funding": {
    "url": "https://github.com/sponsors/finwo"
  },
  "homepage": "https://github.com/finwodb/tank#readme",
  "keywords": [
    "tank",
    "tankdb",
    "tank-db",
    "graph",
    "document",
    "key",
    "value",
    "datastore",
    "database",
    "engine",
    "realtime",
    "decentralized",
    "peer-to-peer",
    "distributed",
    "P2P",
    "level",
    "leveldb",
    "level-db",
    "db",
    "ws",
    "websocket",
    "network",
    "networked"
  ],
  "devDependencies": {
    "aaargh": "^1.0.1",
    "esbuild": "^0.12.19"
  }
}
